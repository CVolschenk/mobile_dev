<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="CCM_Support.TestPage">
  <StackLayout>
    <Button Text="Register" Clicked="Register" x:Name="btnReg"/>
    <Button Text="UnRegister" Clicked="UnRegister" x:Name="btnUnreg"/>
    <Entry x:Name="txtMessage" />
    <Button Text="Send" Clicked="Send_Clicked" x:Name="btnSend"/>
    <ActivityIndicator IsRunning="False" x:Name="Test_Activity"/>
  </StackLayout>
</ContentPage>